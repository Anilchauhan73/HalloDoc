<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - HalloDoc</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/ViewCase.css">  
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.5.0/font/bootstrap-icons.css">
    <link rel="stylesheet" href="~/HalloDoc.styles.css" asp-append-version="true" />
</head>
<body>
    <header class=" d-flex justify-content-between">
        <div>
            <img class="hallodoc-logo img-fluid" src="/images/Fig56. Patient site 1.png" alt="HalloDoc Logo">
        </div>

        <div>
            <span class="header-text d-none d-md-inline">Welcome <strong>Admin Admin2</strong></span>
            <span class="logoutBtn px-2 py-2 m-2 text-decoration-none">
                <i class="bi bi-box-arrow-right me-2"></i>
                <span class="d-md-inline d-none"> Logout</span>
            </span>
            <img class="moon-icon px-3 py-2 m-2" src="/images/moon.svg" alt="Moon">
        </div>
    </header>

    <div class="tab ">
        <button class="tablinks mx-3" onclick="openPage(event, 'dashboard')">Dashboard</button>
        <button class="tablinks mx-3" onclick="openPage(event, 'Provider Location')">Provider Location</button>
        <button class="tablinks mx-3" onclick="openPage(event, 'My Profile')">My Profile</button>
        <button class="tablinks mx-3" onclick="openPage(event, 'Providers')">Providers</button>
        <button class="tablinks mx-3" onclick="openPage(event, 'Partners')">Partners</button>
        <button class="tablinks mx-3" onclick="openPage(event, 'Access')">Access</button>
        <button class="tablinks mx-3" onclick="openPage(event, 'Records')">Records</button>
    </div>


   @RenderBody()
    


    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>

